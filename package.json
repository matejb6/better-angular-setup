{
  "name": "ng-custom-setup",
  "version": "0.2.0",
  "author": {
    "name": "Matej Buljan",
    "email": "matejb6@icloud.com"
  },
  "description": "An Angular app with custom setup",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "prettier:check": "npx prettier --check ./src",
    "prettier:write": "npx prettier --write ./src",
    "test": "ng test --browsers=Chrome",
    "test:ci": "ng test --browsers=ChromeHeadless --reporters=spec --no-watch",
    "test:coverage": "ng test --browsers=ChromeHeadless --reporters=spec --no-watch --code-coverage",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.7",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "^13.3.7",
    "@angular/compiler": "^13.3.7",
    "@angular/core": "^13.3.7",
    "@angular/forms": "^13.3.7",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "^13.3.7",
    "@angular/platform-browser-dynamic": "^13.3.7",
    "@angular/router": "^13.3.7",
    "rxjs": "~6.6.0",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.5",
    "@angular-eslint/builder": "^13.2.1",
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@angular-eslint/schematics": "^13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "^13.3.7",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^14.18.17",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "jasmine-core": "^4.1.1",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-spec-reporter": "0.0.34",
    "prettier": "^2.6.2",
