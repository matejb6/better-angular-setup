{
  "name": "ng-custom-setup",
  "version": "1.2.0",
  "description": "Default Angular app extended with additional libraries and setup to help develop better apps",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "prettier:check": "prettier --check ./src",
    "prettier:write": "prettier --write ./src",
    "lint": "ng lint",
    "test": "ng test --browsers=Chrome",
    "test:ci": "ng test --browsers=ChromeHeadless --reporters=spec --no-watch",
    "test:coverage": "ng test --browsers=ChromeHeadless --reporters=spec --no-watch --code-coverage",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "rxjs": "^7.8.0",
    "tslib": "^2.4.1",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "^14.4.0",
    "@angular-eslint/eslint-plugin-template": "^14.4.0",
    "@angular-eslint/schematics": "^14.4.0",
    "@angular-eslint/template-parser": "^14.4.0",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@types/jasmine": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jasmine-core": "^4.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-spec-reporter": "0.0.34",
    "prettier": "^2.8.3",
    "typescript": "~4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matejb6/ng-custom-setup.git"
  },
  "bugs": {
    "url": "https://github.com/matejb6/ng-custom-setup/issues"
  },
  "keywords": [
    "matejb6",
    "Angular",
    "Custom",
    "Setup",
    "Libraries"
  ],
  "author": {
    "name": "Matej Buljan",
    "email": "matejb6@icloud.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  }
}
